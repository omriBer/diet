# Copy this content to Streamlit Cloud Secrets
# Go to: App Settings > Secrets

# Your app password (change this!)
PASSWORD = "your_secret_password_here"

# GitHub Personal Access Token (for data storage)
# Create at: https://github.com/settings/tokens
# Required scope: "gist"
GITHUB_TOKEN = "ghp_your_github_token_here"

# Gist ID (will be auto-created on first run, then paste here)
GIST_ID = ""
